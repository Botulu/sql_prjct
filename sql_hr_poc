  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "USERS"  ENABLE;
  ALTER TABLE "JOBS" MODIFY ("JOB_TITLE" CONSTRAINT "JOB_TITLE_NN" NOT NULL ENABLE);

  ALTER TABLE "DEPARTMENTS" ADD CONSTRAINT "DEPT_LOC_FK" FOREIGN KEY ("LOCATION_ID")
	  REFERENCES "LOCATIONS" ("LOCATION_ID") ON DELETE SET NULL ENABLE;
  ALTER TABLE "DEPARTMENTS" ADD CONSTRAINT "DEPT_EMP_FK" FOREIGN KEY ("MANAGER_ID")
	  REFERENCES "EMPLOYEES" ("EMPLOYEE_ID") ENABLE;

  ALTER TABLE "EMPLOYEES" ADD CONSTRAINT "EMP_JOB_FK" FOREIGN KEY ("JOB_ID")
	  REFERENCES "JOBS" ("JOB_ID") ENABLE;
  ALTER TABLE "EMPLOYEES" ADD CONSTRAINT "EMP_DEPT_FK" FOREIGN KEY ("DEPARTMENT_ID")
	  REFERENCES "DEPARTMENTS" ("DEPARTMENT_ID") ENABLE;
  ALTER TABLE "EMPLOYEES" ADD CONSTRAINT "EMP_MANAGER_FK" FOREIGN KEY ("MANAGER_ID")
	  REFERENCES "EMPLOYEES" ("EMPLOYEE_ID") ENABLE;

  ALTER TABLE "ORDERS" ADD CONSTRAINT "FK_ORDERS_CUSTOMERS" FOREIGN KEY ("CUSTOMER_ID")
	  REFERENCES "CUSTOMERS" ("CUSTOMER_ID") ON DELETE CASCADE ENABLE;
  ALTER TABLE "ORDERS" ADD CONSTRAINT "FK_ORDERS_EMPLOYEES" FOREIGN KEY ("SALESMAN_ID")
	  REFERENCES "EMPLOYEES" ("EMPLOYEE_ID") ON DELETE SET NULL ENABLE;

  ALTER TABLE "ORDER_ITEMS" ADD CONSTRAINT "FK_ORDER_ITEMS_ORDERS" FOREIGN KEY ("ORDER_ID")
	  REFERENCES "ORDERS" ("ORDER_ID") ON DELETE CASCADE ENABLE;
  ALTER TABLE "ORDER_ITEMS" ADD CONSTRAINT "FK_ORDER_ITEMS_PRODUCTS" FOREIGN KEY ("PRODUCT_ID")
	  REFERENCES "PRODUCTS" ("PRODUCT_ID") ON DELETE CASCADE ENABLE;

/*--
Вывести данные о товарах, у которых столбец rating_p имеет значение 3 или 4,
а price<1000.
--*/
select *
from products
where rating_p in (3,4) and price<1000;

/*--
Вывести first_name, last_name сотрудников, у которых first_name начинается
на букву P и в last_name есть буква r.
--*/
select first_name, last_name
from employees
where first_name like 'P%' and last_name like '%r%';

/*--
Вывести значение столбцов employee_id, department_id, first_name, last_name,
job_id, salary, department_id сотрудников, у которых зарплата salary>9000 и
работают в одном из отделов: 50, 80, 100
--*/
select employee_id, department_id, first_name, last_name, job_id, salary, department_id
from employees
where salary>9000 and department_id in (50,80,100);

/*--
Вывести содержимое столбца street_address в таблице Locations тех строк,
у которых значение этого столбца начинается не с цифры
--*/
select street_address
from locations
where street_address between 'A' and 'Z'

/*--
Вывести значения столбцов employee_id, department_id, first_name, last_name,
job_id, salary, bonus содержит размер премии, которая вычисляется по формуле:
Salary * (1+0.1*rating_e). Выводимые данные упорядочить по размеру премии.
--*/
select employee_id,department_id, first_name, last_name, job_id, salary, (salary*(1+0.1*rating_e)) as bonus
from employees
order by bonus desc;

/*--
Вывести коды руководителей (manager_id), количество и суммарную зарплату сотрудников,
находящихся у них в непосредственном подчинении
--*/
select manager_id, count(employee_id) as empl, sum(salary) as salary
from employees
group by manager_id
order by manager_id;

/*--
Определить должность, для которой разница между максимальной и минимальной
зарплатой минимальна
--*/
select job_id
from jobs
where (max_salary-min_salary)=(select min(max_salary-min_salary)
from jobs);

/*--
Вывести названия городов, имеющих почтовые индексы 00989, 3095, M5V 2L7, 98199,
и названия отделов, расположенных в этих городах.
    Вариант а: вывести только те города, где есть отделы.
    Вариант б: вывести все города с заданными почтовыми индексами.
--*/
select lc.location_id, lc.city,dp.department_name
from departments dp join locations lc on (dp.location_id=lc.location_id)
where lc.location_id in(select location_id
from departments
group by location_id
having count(department_id)>0) and lc.postal_code in ('00989', '3095', 'M5V 2L7', '98199');

/*--
Вывести количество сотрудников и суммарную зарплату сотрудников, работающих в каждом
городе. Должны быть выведены данные о всех городах из таблицы Locations.
--*/
select lc.city, count(city), sum(salary)
from employees el join departments dp on el.department_id=dp.department_id
join locations lc on dp.location_id=lc.location_id
group by city
